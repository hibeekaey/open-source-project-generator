{{if .Components.Frontend.MainApp}}	@$(MAKE) install-app{{end}}
{{if .Components.Frontend.Home}}	@$(MAKE) install-home{{end}}
{{if .Components.Frontend.Admin}}	@$(MAKE) install-admin{{end}}
